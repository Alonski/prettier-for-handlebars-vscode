{
  "name": "handlebars-formatter",
  "displayName": "handlebars-formatter",
  "description": "Prettier formatting for Handlebars files",
  "version": "0.0.5",
  "publisher": "mfeckies",
  "repository": "https://github.com/mfeckie/handlebars-formatter.git",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "copyFixtures": "cp -R ./test-fixtures ./out/test/suite/",
    "pretest": "npm run compile && npm run copyFixtures",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.17.6",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.6",
    "mocha": "^6.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "vscode-test": "^1.2.3"
  },
  "dependencies": {
    "@types/prettier": "^1.19.0",
    "prettier": "^1.19.1"
  }
}

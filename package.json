{
    "name": "prettier-for-handlebars-vscode",
    "displayName": "Prettier for Handlebars",
    "description": "Prettier formatting for Handlebars files",
    "version": "0.0.1",
    "publisher": "alonski",
    "repository": "https://github.com/Alonski/prettier-for-handlebars-vscode.git",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "5.2.7",
        "@types/node": "12.12.27",
        "tslint": "5.18.0",
        "typescript": "3.5.3",
        "vscode": "1.1.36"
    },
    "dependencies": {
        "@types/prettier": "^1.18.1",
        "prettier": "^1.18.2"
    }
}

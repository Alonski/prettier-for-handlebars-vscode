{
  "name": "handlebars-formatter",
  "displayName": "handlebars-formatter",
  "description": "Prettier formatting for Handlebars files",
  "version": "1.1.0",
  "publisher": "mfeckies",
  "repository": "https://github.com/mfeckie/handlebars-formatter.git",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "copyFixtures": "cp -R ./test-fixtures ./out/test/suite/",
    "pretest": "npm run compile && npm run copyFixtures",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.4",
    "@types/prettier": "^2.1.0",
    "@types/vscode": "^1.48.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "tslint": "^6.1.3",
    "typescript": "4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "prettier": "^2.1.1"
  }
}
